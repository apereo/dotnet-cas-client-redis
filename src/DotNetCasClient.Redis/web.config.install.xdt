<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
    <!-- Configure default DotNetCasClient.Redis appSettings variable -->
    <appSettings xdt:Transform="InsertIfMissing" />

    <appSettings>
        <add xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)" key="cas:Redis:ConnectionString" value="127.0.0.1,defaultDatabase=0,abortConnect=false" />
    </appSettings>
</configuration>